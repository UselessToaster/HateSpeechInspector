{
    "name": "Samura",
    "description": "Description of app goes here",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Samura",
        "default_icon": {
            "16": "Sword-icon_16.png",
            "32": "Sword-icon_32.png"
        }
    }
}
